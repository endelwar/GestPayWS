{
  "name": "pepeverde/gestpayws",
  "type": "library",
  "description": "PHP implementation of GestPay (Banca Sella) Web Services",
  "keywords": [
    "gestpay",
    "bancasella",
    "webservice"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Manuel Dalla Lana",
      "email": "endelwar@aregar.it"
    }
  ],
  "support": {
    "issues": "https://github.com/pepeverde/GestPayWS/issues"
  },
  "autoload": {
    "psr-4": {
      "EndelWar\\GestPayWS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EndelWar\\GestPayWS\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6.7",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-soap": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.27 || ^6.5 || ^7",
    "codacy/coverage": "~1.4",
    "php-coveralls/php-coveralls": "^2.1",
    "friendsofphp/php-cs-fixer": "^2.12|^2.13|^2.14",
    "symfony/var-dumper": "^3.4|^4.2",
    "phpstan/phpstan": "^0.11.5"
  }
}
